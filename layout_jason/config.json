        "num_filters": 8,
        "ifmap_offset": 0,
        "ifmap_elem_bytes": 4,
        "filter_offset": 5000,
        "filter_elem_bytes": 4,
        "ofmap_offset": 8000,
        "ofmap_elem_bytes": 4
      },
      "scale_config": {
        "array_rows": 4,
        "array_cols": 4,
        "ifmap_sz_kb": 64,
        "filter_sz_kb": 64,
        "ofmap_sz_kb": 64,
        "df": "ws"
      }
    }
  ]
}