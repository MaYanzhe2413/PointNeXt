      "name": "fps_stage",
      "points_file": "demo_pointnet\\points.npy",
      "params": {
        "num_points": 64,
        "num_samples": 16,
        "point_dims": 3,
        "idx_offset": 0,
        "xyz_offset": 1000,
        "dist_offset": 2000,
        "output_idx_offset": 3000
